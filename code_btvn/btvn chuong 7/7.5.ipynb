    "import tkinter as tk\n",
    "from PIL import Image, ImageTk\n",
    "\n",
    "root = tk.Tk()\n",
    "root.title(\"Xem ảnh\")\n",
    "\n",
    "img = Image.open(\"anh.png\")   # đổi tên ảnh nếu cần\n",
    "img = img.resize((400, 250))\n",
    "photo = ImageTk.PhotoImage(img)\n",
    "\n",
    "label = tk.Label(root, image=photo)\n",
    "label.pack()\n",
    "\n",
    "root.mainloop()"
   ]
