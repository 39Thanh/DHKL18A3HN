   "source": [
    "import tkinter as tk\n",
    "from tkinter import font\n",
    "\n",
    "root = tk.Tk()\n",
    "root.title(\"Font chữ\")\n",
    "\n",
    "f = font.Font(family=\"Arial\", size=16, weight=\"bold\")\n",
    "\n",
    "label = tk.Label(root, text=\"Chữ đậm\", font=f)\n",
    "label.pack(pady=20)\n",
    "\n",
    "root.mainloop()"
   ]
  }
