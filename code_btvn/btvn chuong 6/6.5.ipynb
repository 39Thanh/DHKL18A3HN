    "import threading\n",
    "import time\n",
    "\n",
    "def request(name):\n",
    "    print(\"Sending request to\", name)\n",
    "    time.sleep(1)\n",
    "    print(\"Response from\", name)\n",
    "\n",
    "sites = [\"Google\", \"Yahoo\", \"Facebook\"]\n",
    "\n",
    "for s in sites:\n",
    "    threading.Thread(target=request, args=(s,)).start()"
   ]
  }
