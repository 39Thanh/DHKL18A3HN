    "import threading\n",
    "import time\n",
    "\n",
    "def download(file):\n",
    "    print(\"Downloading\", file)\n",
    "    time.sleep(2)\n",
    "    print(\"Finished\", file)\n",
    "\n",
    "files = [\"file1\", \"file2\", \"file3\"]\n",
    "\n",
    "for f in files:\n",
    "    t = threading.Thread(target=download, args=(f,))\n",
    "    t.start()\n"
   ]
  }
