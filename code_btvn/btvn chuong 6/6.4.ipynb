   "source": [
    "import threading\n",
    "\n",
    "def factorial(n):\n",
    "    result = 1\n",
    "    for i in range(1, n+1):\n",
    "        result *= i\n",
    "    print(f\"Factorial of {n} =\", result)\n",
    "\n",
    "numbers = [5, 6, 7]\n",
    "\n",
    "for n in numbers:\n",
    "    t = threading.Thread(target=factorial, args=(n,))\n",
    "    t.start()"
   ]
  }
