   "source": [
    "import threading\n",
    "\n",
    "def show_name():\n",
    "    print(\"Hello from\", threading.current_thread().name)\n",
    "\n",
    "threads = []\n",
    "\n",
    "for i in range(3):\n",
    "    t = threading.Thread(target=show_name)\n",
    "    threads.append(t)\n",
    "    t.start()\n",
    "\n",
    "for t in threads:\n",
    "    t.join()"
   ]
  }
